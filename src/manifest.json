{
  "name": "NoVote",
  "manifest_version": 2,
  "version": "0.1.46",
  "description": "Hide Reddit's up/downvote buttons, user karma, post scores, comment scores, post comment counts and gilded icons.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.reddit.com/*",
        "https://*.reddit.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "background/index.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "page_action": {
    "default_icon": "images/novote32.png",
    "default_title": "NoVote",
    "default_popup": "./popup/popup.html"
  },
  "icons": {
    "16": "images/novote16.png",
    "32": "images/novote32.png",
    "48": "images/novote48.png",
    "128": "images/novote128.png"
  }
}
